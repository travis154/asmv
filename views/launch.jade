doctype html
html
	head
		meta(name="viewport", content="width=device-width, initial-scale=1.0, user-scalable=no")
		title Asurumaa
		link(href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,300,600,700', rel='stylesheet', type='text/css')
		link(rel='stylesheet', href='/css/bootstrap.min.css')
		link(rel='stylesheet', href='/genericons/genericons.css')
		link(rel='stylesheet', href='/css/app.css')
		script(src='/js/jquery-2.0.3.min.js')			
		style .carousel-inner>.item>img, .carousel-inner>.item>a>img{width:100%}
	body(data-spy="scroll", data-target=".navbar-app", style='text-align:center')
		section.container
			section(style='margin-top: 115px;')
				button.btn.btn-lg.btn-danger(style='font-size: 95px')#l launch
		script.
			$(function(){
				$('body').on('click','#l', function(){
					$("#l").html('<strong style="font-size: 235px">3</strong>');
					setTimeout(function(){
						$("#l").html('<strong style="font-size: 235px">2</strong>');
						setTimeout(function(){
							$("#l").html('<strong style="font-size: 235px">1</strong>');
							setTimeout(function(){
								$("#l").html('<strong style="font-size: 235px">.</strong>');
								window.location = "http://asurumaa.mv";
							},2000);							
						},2000);					
					},2000)
				});
			});